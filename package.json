{
  "name": "timeline",
  "version": "0.0.3",
  "description": "Visualisation for observing encounters by time",
  "author": "NEL",
  "keywords": [
    "d3",
    "TypeScript"
  ],
  "license": "MIT",
  "homepage": "https://github.com/NELCSU/Timeline",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NELCSU/Timeline.git"
  },
  "bugs": {
    "url": "https://github.com/NELCSU/Timeline/issues"
  },
  "scripts": {
    "build": "rollup -cw",
    "css": "npm run css-compile && npm run css-postcss",
    "css-compile": "node-sass src/css --output=docs/dist/css -q -x",
    "css-postcss": "postcss docs/dist/css/**/*.css -r -c postcss.config.js",
    "eslint": "eslint -c .eslintrc.json --ext .ts --ext .js ./src",
    "https": "http-server ./docs -a localhost -p 5000 -o",
    "http": "http-server ./docs -a localhost -p 5000 -o --ssl --key private/key.pem --cert private/cert.pem",
    "test": "npm run https",
    "test.compile": "jest --config jestconfig.json"
  },
  "dependencies": {
    "@buckneri/js-lib-random": "^0.1.4",
    "@buckneri/js-lib-slicer": "0.0.5",
    "d3-array": "^2.4.0"
  },
  "devDependencies": {
    "@types/d3": "^5.7.2",
    "@types/jest": "^25.1.1",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "autoprefixer": "^9.7.4",
    "eslint": "^6.8.0",
    "eslint-plugin-jsdoc": "^21.0.0",
    "jest": "^25.1.0",
    "jsdoc": "^3.6.3",
    "node-sass": "^4.13.1",
    "postcss-cli": "^7.1.0",
    "postcss-sorting": "^5.0.1",
    "rollup": "^1.30.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "stylelint": "^13.0.0",
    "ts-jest": "^25.1.0",
    "typescript": "^3.7.5"
  },
  "main": "rollup.config.js"
}
